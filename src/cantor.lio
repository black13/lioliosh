(fn '(subs old new seq)
    '(select
        '((== (len seq) 0)      nil)
        '((== (len seq) 1)      (if (== (fst seq) old) '(list new) '(seq)))
        '(otherwise             (join (subs old new (head seq))
                                      (subs old new (tail seq))))))

(fn '(cantor1 depth)
    if (depth)
        '(subs '(1) '(1 0 1) (subs '(0) '(0 0 0) (cantor1 (- depth 1))))
        '('(1))